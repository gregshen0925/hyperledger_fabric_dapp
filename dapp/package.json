{
  "name": "backend-template",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "MIT",
  "author": "",
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc",
    "eslint": "node_modules/.bin/eslint ./src/**/*.ts ./test/**/*.ts",
    "lint": "tsc --noEmit && node_modules/.bin/eslint ./src/**/*.ts ./test/**/*.ts --quiet --fix",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "migration:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "migration:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "start": "node dist/server.js",
    "start:dev": "nodemon --watch src -e ts,tsx --exec ts-node --transpile-only src/server.ts",
    "test:dev": "nodemon --watch test -e ts,tsx --exec npm run test",
    "test": "NODE_ENV=testing mocha --reporter spec --require ts-node/register ./test/**/*.test.ts --exit",
    "test:nyan": "NODE_ENV=testing mocha --reporter nyan --require ts-node/register ./test/**/*.test.ts --exit",
    "test:coverage": "nyc npm run test",
    "test_ci": "NODE_ENV=testing mocha --reporter nyan --bail --require ts-node/register ./test/**/*.test.ts --exit",
    "test_ci:coverage": "nyc npm run test_ci"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@koa/cors": "^3.1.0",
    "@koa/router": "^8.0.8",
    "@types/hapi__joi": "^17.1.6",
    "@types/pg": "^7.14.11",
    "bcrypt": "^5.0.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^8.2.0",
    "fabric-network": "^2.2.10",
    "fast-sha256": "^1.3.0",
    "jasmine": "^3.9.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^5.2.0",
    "koa-session": "^6.1.0",
    "koa-static-server": "^1.5.2",
    "koa-swagger-decorator": "^1.6.8",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.31",
    "nodemon": "^2.0.14",
    "pg": "^8.5.1",
    "sha256": "^0.2.0",
    "text-encoding": "^0.7.0",
    "typeorm": "^0.2.38",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/bcrypt": "^3.0.0",
    "@types/crypto-js": "^4.0.2",
    "@types/faker": "^4.1.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa": "2.0.49",
    "@types/koa__cors": "^3.0.1",
    "@types/koa__router": "^8.0.2",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-helmet": "^5.2.2",
    "@types/koa-session": "^5.10.3",
    "@types/koa-static-server": "^1.3.0",
    "@types/mocha": "^8.2.3",
    "@types/node": "^14.6.4",
    "@types/sinon": "^9.0.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "sinon": "^9.2.0",
    "ts-node": "^8.10.2",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^3.9.7"
  }
}
